{
  "name": "baum/baum",
  "type": "library",
  "description": "Baum is an implementation of the Nested Set pattern for Eloquent models.",
  "keywords": ["nested set", "laravel", "laravel 5", "eloquent", "database"],
  "license": "MIT",
  "authors": [
    {
      "name": "Estanislau Trepat",
      "email": "estanis@etrepat.com",
      "homepage": "https://etrepat.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/console": "^9.0",
    "illuminate/database": "^9.0",
    "illuminate/events": "^9.0",
    "illuminate/filesystem": "^9.0",
    "illuminate/support": "^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "mockery/mockery": "^1.4.4"
  },
  "autoload": {
    "psr-4": {
      "Baum\\": "src/"
    }
  },
  "autoload-dev": {
      "files": [
        "tests/suite/Support/helpers.php"
      ],
      "psr-4": {
          "Baum\\Tests\\": "tests/suite/"
      }
  },
  "extra": {
    "laravel": {
        "providers": [
          "Baum\\BaumServiceProvider"
        ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
